*
	margin 0
	padding 0
	outline none
body
	font-size: 18px
	font-family: OS
	color #000
	background-color #fff
	line-height: 1.5
	@media screen and (min-width 2295px)
		font-size: 20px
.container
	width 100%
	max-width 1200px
	position relative
	@media screen and (min-width 2295px)
		max-width 1400px
a
	color: #000
	&:focus
		outline: 0
		text-decoration none
	&:hover
		text-decoration none
h1, h2, h3, h4, h5, h6
	font-weight normal
	margin 0
button
	border none
section
header
footer
	width 100%
	display block
.wrapper
	overflow hidden
.video-wrapper
	margin 0 auto
	position relative
	width 100%
	top 0
	// z-index 60
	&:before
		content ''
		height 0
		padding-top 56.25%
		display block
	&:after
		content: ''
		display block
		position: absolute
		top: 0
		left 0
		right 0
		width: 100%
		height 70px
	img,
	iframe
		position absolute
		width 100%
		height 100%
		display block
		background-color #000
		border-radius 0
		border: 0
		top 0
		left 0
		right 0
		bottom 0
		cursor pointer
		margin 0 auto